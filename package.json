{
  "name": "github-releases-feed",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "yarn@4.9.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "types": "yarn types:typescript; yarn types:svelte",
    "types:typescript": "tsc --noEmit",
    "types:svelte": "svelte-check --output=human",
    "lint": "yarn lint:oxlint; yarn lint:eslint",
    "lint:oxlint": "time oxlint --type-aware src/**/*.{js,ts,svelte}",
    "lint:eslint": "time eslint --concurrency=auto src/**/*.{js,ts,svelte}",
    "format": "prettier --write index.html src/**/*.{js,ts,svelte}"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@octokit/core": "^7.0.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/svelte": "^5.0.5",
    "date-fns": "^4.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.14.0",
    "eslint-plugin-svelte": "^3.12.1",
    "globals": "^16.3.0",
    "idb": "^8.0.3",
    "lightningcss": "^1.30.1",
    "oxlint": "^1.14.0",
    "oxlint-tsgolint": "^0.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.7",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.3.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4"
  }
}
