{
  "name": "github-releases-feed",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "types": "pnpm types:typescript; pnpm types:svelte",
    "types:typescript": "tsc --noEmit",
    "types:svelte": "svelte-check --output=human",
    "lint": "pnpm lint:oxlint; pnpm lint:eslint",
    "lint:oxlint": "oxlint --type-aware src/**/*.{js,ts,svelte}",
    "lint:eslint": "eslint --concurrency=auto src/**/*.{js,ts,svelte}",
    "format": "prettier --write index.html src/**/*.{js,ts,svelte}"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@octokit/core": "7.0.5",
    "@octokit/graphql": "9.0.2",
    "@octokit/request-error": "7.0.1",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@tsconfig/svelte": "5.0.5",
    "date-fns": "4.1.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-oxlint": "1.24.0",
    "eslint-plugin-svelte": "3.12.5",
    "globals": "16.4.0",
    "idb": "8.0.3",
    "lightningcss": "1.30.2",
    "oxlint": "1.24.0",
    "oxlint-tsgolint": "0.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.41.2",
    "svelte-check": "4.3.3",
    "svelte-eslint-parser": "1.4.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.11"
  }
}
